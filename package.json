{
  "name": "aintivirus-extension",
  "version": "0.1.0",
  "description": "AINTIVIRUS - Browser security extension",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "build:firefox": "webpack --mode production --env target=firefox",
    "clean": "rimraf dist",
    "compile:rulesets": "node scripts/compile-rulesets.js",
